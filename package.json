{
    "name": "@nbb.com/ts-json-loader",
    "version": "1.0.3",
    "description": "Load object properties from JSON files to existing objects",
    "main": "index.js",
    "scripts": {
        "pretest": "tsc",
        "test": "node ./node_modules/mocha/bin/_mocha --timeout 999999 --colors ./test/_suite.js",
        "postinstall": "node -e \"require('@nbb.com/npmbuildtools/lib/check-global-deps').CheckGlobalDeps(['typescript'])\"",
        "prepare": "tsc",
        "postpack": "node -e \"require('@nbb.com/npmbuildtools').PostPack([ [ './node_modules/@nbb.com/npmbuildtools/lib/clean-package-elements', 'scripts.postpack', 'scripts.test', 'scripts.pretest', 'scripts.postinstall', 'directories.test' ], [ './node_modules/@nbb.com/npmbuildtools/lib/create-fallback-readme' ] ], { verbose: true })\""
    },
    "keywords": [
        "typescript",
        "JSON"
    ],
    "author": "Thorben Wolkersdorfer <thorbenw@nbb.com> (https://www.notebooksbilliger.de/)",
    "license": "MIT",
    "repository": "github:notebooksbilliger/ts-json-loader",
    "readme": "README.adoc",
    "engines": {
        "node": ">=8.17.0"
    },
    "directories": {
        "doc": "./doc",
        "test": "./test"
    },
    "dependencies": {
        "@nbb.com/npmbuildtools": "^2.0.0",
        "fs-extra": "^8.1.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^8.1.0",
        "@types/mocha": "^7.0.1",
        "mocha": "^7.1.0"
    }
}
